class CLessThen9 {
public:
	bool operator()(int n) {
		return n < 9;
	}
};
void outputSquare(int value) {
	cout << value * value << "";
}
int calculateCube(int value) {
	return value * value * value;
}
int main(){
	const int SIZE= 10;

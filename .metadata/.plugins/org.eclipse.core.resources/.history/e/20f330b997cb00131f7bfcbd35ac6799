#include <iostream>
#include <map> //使用multimap需要包含此头文件#include <string>
using namespace std;

class CStudent {
public:
	struct CInfo {
		int id;
		string name;
	};
	int score;
	CInfo info;
};
typedef multimap<int, CStudent::CInfo> MAP_STD;
int main() {
	MAP_STD mp;
	CStudent st;
	string cmd;
	while (cin >> cmd) {
		if (cmd == "Add") {
			cin >> st.info.name >> st.info.id >> st.score;
		} else if (cmd == "Query") {

		}
	}

	return 0;
}
